{
  "name": "vola-stage1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-setup": "node scripts/check-setup.js",
    "init-db": "node scripts/init-db.js",
    "test-db": "node scripts/test-db.js",
    "debug-db": "node scripts/debug-db.js",
    "audit-fix": "npm audit fix --force",
    "setup": "node scripts/setup.js",
    "clean": "rm -rf .next node_modules package-lock.json && npm install",
    "create-admin": "node scripts/create-admin.js",
    "init-quiz": "node scripts/init-quiz-tables.js",
    "init-rag": "node scripts/init-rag-db.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "docker:start": "powershell -ExecutionPolicy Bypass -File ./scripts/docker-db.ps1 start",
    "docker:stop": "powershell -ExecutionPolicy Bypass -File ./scripts/docker-db.ps1 stop",
    "docker:status": "powershell -ExecutionPolicy Bypass -File ./scripts/docker-db.ps1 status",
    "docker:logs": "powershell -ExecutionPolicy Bypass -File ./scripts/docker-db.ps1 logs",
    "docker:reset": "powershell -ExecutionPolicy Bypass -File ./scripts/docker-db.ps1 reset",
    "process-pdfs": "npx tsx scripts/process-pdfs.ts",
    "process-pdfs:dry-run": "npx tsx scripts/process-pdfs.ts --dry-run",
    "process-pdfs:verbose": "npx tsx scripts/process-pdfs.ts --verbose --report-file processing-report.json"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@types/bcryptjs": "2.4.6",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "22.9.0",
    "@types/pg": "8.11.10",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@xenova/transformers": "^2.17.2",
    "autoprefixer": "10.4.20",
    "bcryptjs": "2.4.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dotenv": "16.4.5",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "0.460.0",
    "next": "15.0.3",
    "pdf-parse": "^2.4.5",
    "pg": "8.13.1",
    "postcss": "8.4.49",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.53.2",
    "recharts": "2.13.3",
    "tailwind-merge": "2.5.4",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint": "9.6.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "9.15.0",
    "eslint-config-next": "15.0.3",
    "fast-check": "^4.5.3",
    "jsdom": "^28.1.0",
    "tsx": "^4.21.0",
    "vitest": "^1.0.4"
  },
  "overrides": {
    "micromatch": "4.0.8",
    "braces": "3.0.3",
    "ws": "8.18.0",
    "path-to-regexp": "6.3.0"
  }
}
